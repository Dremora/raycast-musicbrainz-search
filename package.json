{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "musicbrainz",
  "title": "MusicBrainz Search",
  "description": "Search MusicBrainz for artists, release groups and recordings.",
  "icon": "command-icon.svg",
  "author": "Dremora",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "artist",
      "title": "Search Artists",
      "subtitle": "MusicBrainz",
      "description": "Search MusicBrainz artists",
      "mode": "view"
    },
    {
      "name": "release-group",
      "title": "Search Release Groups",
      "subtitle": "MusicBrainz",
      "description": "Search MusicBrainz release groups",
      "mode": "view"
    },
    {
      "name": "recording",
      "title": "Search Recordings",
      "subtitle": "MusicBrainz",
      "description": "Search MusicBrainz recordings",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.13.3",
    "ky": "^1.2.2",
    "musicbrainz-api": "^0.14.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
